{
  "version": 2,
  "builds": [
    {
      "src": "build.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "node build.js"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/public/$1"
    }
  ],
  "outputDirectory": "dist/public"
}
